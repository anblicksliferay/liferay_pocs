<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" default-lazy="false">
	<import class="com.delphi.userprofile.model.AdminTasks" />
	<import class="com.delphi.userprofile.model.UserAccessRequest" />
	<import class="com.delphi.userprofile.model.UserProfile" />
	<class name="com.delphi.userprofile.model.impl.AdminTasksImpl" table="Delphi_AdminTasks">
		<composite-id class="com.delphi.userprofile.service.persistence.AdminTasksPK" name="primaryKey">
			<key-property name="adminTaskId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
			<key-property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		</composite-id>
		<property column="uuid_" name="uuid" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="systemAlert" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
		<property name="messageText" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="postOnPortBanner" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
		<property name="siteAlert" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
		<property name="siteName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="createdById" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
	</class>
	<class name="com.delphi.userprofile.model.impl.UserAccessRequestImpl" table="Delphi_UserAccessRequest">
		<id name="requestId" type="long">
			<generator class="assigned" />
		</id>
		<property column="uuid_" name="uuid" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="createdById" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
		<property name="portletAccess" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="requestType" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="responseType" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="responseComment" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="responseUser" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="activeState" type="com.liferay.portal.dao.orm.hibernate.IntegerType" />
	</class>
	<class name="com.delphi.userprofile.model.impl.UserProfileImpl" table="Delphi_UserProfile">
		<id name="profileId" type="long">
			<generator class="assigned" />
		</id>
		<property column="uuid_" name="uuid" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="createdById" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
		<property name="communicationPreferences" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="region" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="site" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="server" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="division" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="language" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="timezone" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="workHours" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="jobrole" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="interests" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="portlets" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
</hibernate-mapping>