<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_0_0.dtd">

<service-builder package-path="com.delphi.userprofile">
	<namespace>Delphi</namespace>
	<entity local-service="true" name="UserProfile" remote-service="true" uuid="true">

		<!-- PK fields -->

		<column name="profileId" primary="true" type="long" />
		<column name="userId" type="long" />

		<!-- Audit fields -->

		<column name="companyId" type="long" />
		<column name="createdById" type="long" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->


		<column name="communicationPreferences" type="String" />
		<column name="region" type="String" />
		<column name="site" type="String" />
		<column name="server" type="String" />
		<column name="division" type="String" />
		<column name="language" type="String" />
		<column name="timezone" type="String" />
		<column name="workHours" type="String" />
		<column name="jobrole" type="String" />
		<column name="interests" type="String" />
		<column name="portlets" type="String" />

		<!-- Order -->

		<order by="asc">
			<order-column name="profileId" />
		</order>

		<!-- Finder methods -->

		<finder name="JobRole" return-type="Collection">
			<finder-column name="jobrole" />
		</finder>
		<finder name="UserId" return-type="UserProfile">
			<finder-column name="userId" />
		</finder>

	</entity>
	
	<entity local-service="true" name="UserAccessRequest" remote-service="true" uuid="true">

		<!-- PK fields -->

		<column name="requestId" primary="true" type="long" />
		<column name="userId" type="long" />

		<!-- Audit fields -->

		<column name="companyId" type="long" />
		<column name="createdById" type="long" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->


		<column name="portletAccess" type="String" />
		<column name="requestType" type="String" />
		<column name="responseType" type="String" />
		<column name="responseComment" type="String" />
		<column name="responseUser" type="long" />
		<column name="activeState" type="int" />

		<!-- Order -->

		<order by="asc">
			<order-column name="requestId" />
		</order>

		<!-- Finder methods -->

		<finder name="UserId" return-type="Collection">
			<finder-column name="userId" />
		</finder>

		<finder name="ActiveState" return-type="Collection">
			<finder-column name="activeState" />
		</finder>

	</entity>
	
	
	<entity  local-service="true" name="AdminTasks" remote-service="true" uuid="true">
	<column name="adminTaskId" type="long" primary="true"></column>  <!-- auto increment -->
	<column name="userId" type="long" primary="true"></column>
	
	<!-- Other fields -->

	<column name="systemAlert" type="boolean"></column>
	<column name="messageText" type="String"></column>
	<column name="postOnPortBanner" type="boolean"></column>
	<column name="siteAlert" type="boolean"></column>
	<column name="siteName" type="String"></column>
	
		<!-- Audit fields -->

		<column name="companyId" type="long" />
		<column name="createdById" type="long" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />
		<order by="asc">
			<order-column name="adminTaskId" />
		</order>
	</entity>
</service-builder>