<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" default-lazy="false">
	<import class="com.ddt.model.FAQSAshok" />
	<import class="com.ddt.model.Loc_Course_Info" />
	<import class="com.ddt.model.NewsAshok" />
	<import class="com.ddt.model.TestimonialAshok" />
	<class name="com.ddt.model.impl.FAQSAshokImpl" table="WDDTS_FAQSAshok">
		<id column="id_" name="id" type="long">
			<generator class="assigned" />
		</id>
		<property column="uuid_" name="uuid" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="FAQSContent" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
	<class name="com.ddt.model.impl.Loc_Course_InfoImpl" table="WDDTS_Loc_Course_Info">
		<id column="id_" name="id" type="long">
			<generator class="assigned" />
		</id>
		<property column="uuid_" name="uuid" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="locationId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="couserId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="locationName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="couserName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="maxSeats" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="toDate" type="org.hibernate.type.TimestampType" />
		<property name="fromDate" type="org.hibernate.type.TimestampType" />
		<property name="Fees" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
	</class>
	<class name="com.ddt.model.impl.NewsAshokImpl" table="WDDTS_NewsAshok">
		<id column="id_" name="id" type="long">
			<generator class="assigned" />
		</id>
		<property column="uuid_" name="uuid" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="Newscontent" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
	<class name="com.ddt.model.impl.TestimonialAshokImpl" table="WDDTS_TestimonialAshok">
		<id column="id_" name="id" type="long">
			<generator class="assigned" />
		</id>
		<property column="uuid_" name="uuid" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="TestimonialContent" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
</hibernate-mapping>