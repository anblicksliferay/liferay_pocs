<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" default-lazy="false">
	<import class="ddtii.model.category" />
	<import class="ddtii.model.course" />
	<import class="ddtii.model.loc_cou_cat" />
	<import class="ddtii.model.location" />
	<class name="ddtii.model.impl.categoryImpl" table="DDTII_category">
		<id name="categoryId" type="long">
			<generator class="assigned" />
		</id>
		<property name="categoryName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="courseId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="locationId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
	</class>
	<class name="ddtii.model.impl.courseImpl" table="DDTII_course">
		<id name="courseId" type="long">
			<generator class="assigned" />
		</id>
		<property name="courseName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="locationId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
	</class>
	<class name="ddtii.model.impl.loc_cou_catImpl" table="DDTII_loc_cou_cat">
		<id name="loc_cou_catId" type="long">
			<generator class="assigned" />
		</id>
		<property name="categoryId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="courseId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="locationId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="durationString" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="durationLong" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="startDate_" type="org.hibernate.type.TimestampType" />
		<property name="endDate_" type="org.hibernate.type.TimestampType" />
		<property name="maxSeats" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
	</class>
	<class name="ddtii.model.impl.locationImpl" table="DDTII_location">
		<id name="locationId" type="long">
			<generator class="assigned" />
		</id>
		<property name="locationName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
	</class>
</hibernate-mapping>