<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_0_0.dtd">

<service-builder package-path="com.astra.workflowservice">
	<namespace>Astra</namespace>
	<entity local-service="true" name="Registration" remote-service="true" uuid="true">
		<column name="RegistrationId" primary="true" type="long" />
        <column name="firstname" type="String"></column>
        <column name="lastname" type="String"></column>
        <column name="course" type="String"></column>
        <column name="email" type="String"></column>
        <column name="location" type="String"></column>
        <column name="status" type="String"></column>
        <column name="comments" type="String"></column>
        <column name="Workflowstepnumber" type="String"></column>
        <column name="WorkflowId" type="String"></column>
        <column name="CurrentApprover" type="String"></column>
		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />
	</entity>
	<entity local-service="true" name="WorkFlowConfig" remote-service="true" uuid="true">
		<column name="WfcId" primary="true" type="long" />
        <column name="ConfigKey" type="String"></column>
        <column name="Content" type="String"></column>
		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />
	</entity>
</service-builder>