<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" default-lazy="false">
	<import class="com.ys.hma.account.model.Book" />
	<import class="com.ys.hma.account.model.Data" />
	<class name="com.ys.hma.account.model.impl.BookImpl" table="Book">
		<id name="bookId" type="long">
			<generator class="increment" />
		</id>
		<property name="bookName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="description" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="authorName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="isbn" type="com.liferay.portal.dao.orm.hibernate.IntegerType" />
		<property name="price" type="com.liferay.portal.dao.orm.hibernate.IntegerType" />
	</class>
	<class name="com.ys.hma.account.model.impl.DataImpl" table="Data">
		<id name="DataId" type="long">
			<generator class="increment" />
		</id>
		<property name="StartDate" type="org.hibernate.type.TimestampType" />
		<property name="EndDate" type="org.hibernate.type.TimestampType" />
		<property name="Exported" type="com.liferay.portal.dao.orm.hibernate.IntegerType" />
		<property name="NotExported" type="com.liferay.portal.dao.orm.hibernate.IntegerType" />
	</class>
</hibernate-mapping>